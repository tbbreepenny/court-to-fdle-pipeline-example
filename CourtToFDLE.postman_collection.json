        "description": "Retrieve FDLE cases filtered by status and county."
      },
      "response": [],
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});"
            ]
          }
        }
      ]
    },
    {
      "name": "FDLE - Get Case by Id",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/fdle/cases/1",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "fdle",
            "cases",
            "1"
          ]
        },
        "description": "Retrieve a single FDLE case by its internal Id (from the database). Update the Id as needed."
      },
      "response": [],
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test(\"Status code is 200 or 404\", function () {",
