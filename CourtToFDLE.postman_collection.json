      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/fdle/cases?status=OPEN&county=Orange",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "fdle",
            "cases"
          ],
          "query": [
            {
              "key": "status",
